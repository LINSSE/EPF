  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-CcKyR1uPLLjOyD7iykX1vA" name="obtencin_de_requisitos,_KjAeAOZTEeeoX-3oEkoa6Q" guid="-CcKyR1uPLLjOyD7iykX1vA">
    <refinedDescription>&lt;p style=&quot;MARGIN-BOTTOM: 0pt; TEXT-ALIGN: justify; MARGIN-LEFT: 7.133pt; MARGIN-TOP: 0pt; LINE-HEIGHT: 1.38&quot; dir=&quot;ltr&quot;>&#xD;
